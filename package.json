    "unique-names-generator": "^4.2.0"
  },
  "devDependencies": {
    "@rollup/plugin-multi-entry": "^3.0.0",
    "concurrently": "^5.0.2",
    "eslint": "^6.8.0",
    "node-cron": "^2.0.3",
    "rollup": "^1.29.0"
  }
}
